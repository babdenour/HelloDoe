{
  "extends": "standard",
  "globals": {
    "before": true,
    "beforeEach": true,
    "afterEach": true,
    "after": true,
    "describe": true,
    "it": true
  },
  "rules": {
    "no-mixed-spaces-and-tabs": [
      "error",
      "smart-tabs"
    ],
    "generator-star-spacing": "off",
    "space-before-function-paren": "off"
  }
}