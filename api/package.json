{
  "name": "hello-doe-api",
  "version": "1.0.0",
  "description": "Backend for Hello Doe",
  "main": "main.js",
  "scripts": {
    "clean": "rimraf dist",
    "cmd": "CLI_PATH=./libs/cli/cli.ts nestjs-command",
    "build": "nest build",
    "build:prod": "nest build -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "heroku-postbuild": "npm run build:prod",
    "postinstall": "./hooks/postinstall.sh",
    "prebuild": "rimraf dist",
    "start": "nest start",
    "start:dev": "npm run clean && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:dist": "node dist/main.js",
    "start:prod": "nest start -p tsconfig.build.json",
    "test": "jest --config config/jest/jest.config.js -i --forceExit --detectOpenHandles",
    "test:all": "npm run test && npm run test:e2e",
    "test:clear": "jest --clearCache",
    "test:coverage": "jest --config config/jest/jest.config.js -i --coverage",
    "test:watch": "jest --config config/jest/jest.config.js -i --watch",
    "test:cov": "jest --config config/jest/jest.config.js --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config config/jest/jest.config.js -i",
    "test:e2e": "jest --config config/jest/jest-e2e.config.js -i --forceExit --detectOpenHandles",
    "test:e2e:watch": "jest --config config/jest/jest-e2e.config.js -i --watch"
  },
  "author": "Baboo7",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "10.16.3",
    "npm": "6.11.3"
  },
  "dependencies": {
    "@nestjs/cli": "^7.5.4",
    "@nestjs/common": "^7.6.5",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.6.5",
    "@nestjs/mongoose": "^7.2.1",
    "@nestjs/platform-express": "^7.6.5",
    "@nestjs/schematics": "^7.2.6",
    "@types/express": "^4.17.1",
    "@types/mongoose": "^5.7.31",
    "accesscontrol": "^2.2.1",
    "body-parser": "^1.17.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "date-fns": "^2.19.0",
    "express": "^4.15.4",
    "express-http-context": "^1.0.4",
    "lodash": "^4.17.11",
    "mongoose": "^5.10.3",
    "nestjs-i18n": "^8.0.11",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "stripe": "^8.137.0",
    "uuid": "^3.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.6.5",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.13",
    "@types/lodash": "^4.14.119",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.7",
    "@types/supertest": "^2.0.8",
    "@types/winston": "^2.4.4",
    "@types/yargs": "^15.0.12",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "array-union": "^2.1.0",
    "chai": "^4.1.2",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-standard": "^5.0.0",
    "faker": "^5.2.0",
    "jest": "^26.4.2",
    "jest-junit": "^12.0.0",
    "nestjs-command": "^2.0.0",
    "prettier": "^2.1.2",
    "rimraf": "^2.6.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.3"
  }
}
