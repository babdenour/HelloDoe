{
  "name": "hello-doe-web-app",
  "version": "1.0.0",
  "description": "Web application for Hello Doe",
  "author": "Baboo7",
  "private": true,
  "scripts": {
    "build": "npm run clean && cross-env webpack --config ./conf/webpack.config.js --hide-modules",
    "build:prod": "npm run clean && cross-env NODE_ENV=production webpack --config ./conf/webpack.config.js --hide-modules",
    "clean": "rm -rf ./dist",
    "clean:unit": "rimraf tests/reports/coverage",
    "lint": "eslint --ext .js,.vue src test/unit",
    "lint:fix": "npm run lint -- --fix",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --inline --progress --config ./conf/webpack.config.js --host localhost --port 8080 --open --history-api-fallback --hot",
    "test": "npm run unit",
    "test:watch": "npm run unit -- --watch",
    "unit": "jest --config ./jest.config.js --errorOnDeprecated -w 1"
  },
